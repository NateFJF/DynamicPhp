{% extends 'base.html.twig' %}
{% block content %}
  <h1>About Us</h1>
  <ul class="team">
    {% for m in team %}
      <li class="member">
        <img src="/images/{{ m.photo }}" alt="{{ m.name }}" />
        <div>
          <h3>{{ m.name }}</h3>
          <p>{{ m.bio }}</p>
        </div>
      </li>
    {% endfor %}
  </ul>
{% endblock %}